#include <iostream>
#include <string>
#include <vector>

using namespace std;

bool IsPalindrom(string s) {
    for (size_t i = 0; i < s.size() / 2; ++i) {
        if (s[i] != s[s.size() - i - 1]) {
            return false;
        }
    }
    return true;
}

int main() {
    string s;
    cin >> s;
    if(IsPalindrom(s)==1){
    	cout << "true";
    }
    else{
    	cout << "false";
    }
	return 0;
}
